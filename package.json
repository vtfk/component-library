{
  "name": "@vtfk/components",
  "version": "0.0.9",
  "description": "Komponentbibliotek for Vestfold og Telemark fylkeskommune",
  "keywords": [],
  "author": "Vestfold og Telemark fylkeskommune (https://github.com/vtfk)",
  "license": "MIT",
  "source": "src/index.js",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vtfk/component-library.git"
  },
  "bugs": {
    "url": "https://github.com/vtfk/component-library/issues"
  },
  "homepage": "https://github.com/vtfk/component-library#readme",
  "scripts": {
    "start": "node server.js",
    "dev": "cross-env STORYBOOK_DEBUG=true SBCONFIG_CONFIG_DIR=scripts/storybook start-storybook -p 4000 --quiet --ci",
    "build": "rollup -c ./scripts/build/rollup.config.js",
    "build-watch": "npm run build -- -w",
    "build-documentation": "build-storybook -c scripts/storybook -o storybook-static",
    "publish-documentation": "storybook-to-ghpages --out=storybook-static --existing-output-dir=storybook-static",
    "start-documentation": "npx http-server storybook-static",
    "test:dev": "jest --watch",
    "test": "jest",
    "release": "npm run build && standard-version",
    "lint": "node node_modules/eslint/bin/eslint.js --fix --max-warnings=0 src",
    "generate": "node --experimental-modules node_modules/.bin/plop --plopfile scripts/templates/index.js",
    "update-packages": "npx npm-check-updates -u && npm i",
    "heroku-postbuild": "npm run build-documentation",
    "now-build": "npm run build-documentation"
  },
  "scriptsComments": {
    "precommit": "lint-staged"
  },
  "dependencies": {
    "nanoid": "^3.1.20",
    "react-datepicker": "^3.4.1",
    "react-scrolllock": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.12.16",
    "@babel/plugin-proposal-object-rest-spread": "7.12.13",
    "@babel/preset-env": "7.12.16",
    "@babel/preset-react": "7.12.13",
    "@rollup/plugin-commonjs": "13.0.0",
    "@rollup/plugin-node-resolve": "11.1.1",
    "@storybook/addon-actions": "6.1.17",
    "@storybook/addon-docs": "6.1.17",
    "@storybook/addon-info": "5.3.21",
    "@storybook/addon-knobs": "6.1.17",
    "@storybook/addon-storysource": "6.1.17",
    "@storybook/addons": "6.1.17",
    "@storybook/preset-scss": "1.0.3",
    "@storybook/react": "6.1.17",
    "@storybook/theming": "6.1.17",
    "@svgr/rollup": "5.5.0",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "10.3.0",
    "@testing-library/react-hooks": "3.3.0",
    "@whitespace/storybook-addon-html": "4.2.0",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.2",
    "babel-plugin-inline-react-svg": "1.1.2",
    "babel-polyfill": "6.26.0",
    "compression": "1.7.4",
    "cross-env": "7.0.3",
    "eslint": "7.19.0",
    "eslint-config-react-app": "5.2.1",
    "eslint-plugin-flowtype": "5.1.3",
    "eslint-plugin-import": "2.21.2",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "4.0.4",
    "express": "4.17.1",
    "husky": "4.2.5",
    "jest": "26.0.1",
    "jest-junit": "11.0.0",
    "jest-transform-css": "2.1.0",
    "lint-staged": "10.2.11",
    "marked": "2.0.0",
    "node-sass": "4.14.1",
    "plop": "2.7.4",
    "prettier": "2.0.5",
    "prop-types": "15.7.2",
    "react-hooks-testing-library": "0.6.0",
    "react-test-renderer": "16.13.1",
    "rimraf": "3.0.2",
    "rollup": "2.18.0",
    "rollup-plugin-auto-external": "2.0.0",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-postcss": "3.1.2",
    "rollup-plugin-svg": "2.0.0",
    "rollup-plugin-terser": "6.1.0",
    "sass-loader": "8.0.2",
    "standard-version": "8.0.0",
    "storybook-readme": "5.0.9",
    "svg-jest": "1.0.1"
  },
  "peerDependencies": {
    "prop-types": ">=15.7.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread"
    ],
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "prettier": {
    "jsxBracketSameLine": true,
    "printWidth": 120,
    "semi": false,
    "singleQuote": true
  },
  "jest": {
    "rootDir": "src",
    "setupFilesAfterEnv": [
      "<rootDir>/../scripts/test/jest-setup.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/../scripts/test/file.mock.js",
      "\\.(css|less|scss)$": "<rootDir>/../scripts/test/style.mock.js"
    },
    "transform": {
      ".+\\.js$": "babel-jest",
      ".+\\.svg$": "svg-jest",
      ".+\\.(css|styl|less|sass|scss)$": "jest-transform-css",
      ".+\\.(bmp|gif|jpg|jpeg|png|psd|webp)$": "<rootDir>/../scripts/test/media-file.mock.js"
    },
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": "<rootDir>/../scripts/test"
        }
      ]
    ],
    "verbose": true
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "rules": {
      "no-console": 1,
      "react/default-props-match-prop-types": 1,
      "react/no-unused-prop-types": 1,
      "react/prop-types": 1,
      "react/sort-prop-types": 1
    },
    "settings": {
      "react": {
        "version": "latest"
      }
    }
  },
  "lint-staged--Comments": {
    "**/*.{js,json,css}": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.js": [
      "npm run lint",
      "git add"
    ]
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "husky--Comments": {
    "hooks": {
      "post-checkout": "if [[ $HUSKY_GIT_PARAMS  =~ 1$ ]]; then npm install --no-package-lock; fi",
      "post-merge": "npm install --no-package-lock",
      "post-rebase": "npm install",
      "pre-commit": "npm run test && lint-staged"
    }
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
